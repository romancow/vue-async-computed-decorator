{
	"name": "@romancow/vue-async-computed-decorator",
	"version": "1.0.0",
	"description": "Decorator and typescript support for the `vue-async-computed` module",
	"main": "build/index.js",
	"repository": "https://github.com/romancow/vue-async-computed-decorator.git",
	"author": "Sean Wilson",
	"license": "MIT",
	"private": false,
	"keywords": ["vue", "async", "computed", "decorator", "typescript"],
	"files": ["build", "types"],
	"scripts": {
		"clean": "rimraf ./build ./types",
		"copy:defs": "ncp ./src ./types --filter='src(/.*\\.d\\.ts)?$'",
		"build:ts": "tsc -p .",
		"prepare": "npm run build:ts && npm run copy:defs",
		"build": "npm run clean && npm run prepare"
	},
	"devDependencies": {
		"ncp": "^2.0.0",
		"rimraf": "^2.6.3",
		"typescript": "^3.5.3",
		"vue": "^2.6.10"
	},
	"peerDependencies": {
		"vue": "~2"
	},
	"dependencies": {
		"vue-async-computed": "^3.7.0",
		"vue-class-component": "^7.1.0"
	}
}
